{
    "base": "semantic/",
    "autoInstall": true,
    "version": "2.2.7",
    "paths": {
        "source": {
            "config": "src/theme.config",
            "definitions": "src/definitions/",
            "site": "src/site/",
            "themes": "src/themes/"
        },
        "output": {
            "packaged": "dist/",
            "uncompressed": "dist/components/",
            "compressed": "dist/components/",
            "themes": "dist/themes/"
        },
        "clean": "dist/"
    },
    "permission": false,
    "rtl": false,
    "components": [
        "reset",
        "site",
        "button",
        "container",
        "divider",
        "header",
        "icon",
        "image",
        "input",
        "label",
        "list",
        "loader",
        "segment",
        "step",
        "form",
        "grid",
        "menu",
        "message",
        "table",
        "card",
        "comment",
        "feed",
        "item",
        "checkbox",
        "dropdown",
        "embed",
        "modal",
        "popup",
        "transition",
        "api",
        "state",
        "visibility",
        "reveal"
    ],
    "redundantComponents": [
        "rail",
        "ad",
        "statistic",
        "accordion",
        "dimmer",
        "nag",
        "progress",
        "rating",
        "shape",
        "search",
        "sidebar",
        "sticky",
        "tab",
        "breadcrumb"
      ],
    "admin": false
}