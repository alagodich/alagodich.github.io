{
  "name": "alagodich.github.io",
  "version": "2.0.2",
  "description": "Personal blog.",
  "main": "index.html",
  "scripts": {
    "start": "JEKYLL_ENV=development bundle exec jekyll serve --draft",
    "lint": "eslint --config ./.eslintrc.json --ext .js,.jsx,.ts,tsx client",
    "test": "jest --errorOnDeprecated --logHeapUsage --verbose",
    "test:watch": "npm run test -- --watch",
    "build": "rm -rf ./public && webpack --config webpack.config.production.js",
    "build:watch": "rm -rf ./public && webpack --config webpack.config.dev.js",
    "fomantic": "gulp build-fomantic",
    "build:all": "npm run fomantic && npm run lint && npm run test && npm run build",
    "browser-sync": "browser-sync start --proxy localhost:4000 --files _site/**"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alagodich/alagodich.github.io.git"
  },
  "author": "alagodich",
  "license": "ISC",
  "homepage": "https://github.com/alagodich/alagodich.github.io#readme",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@kunstmusik/csound": "^6.15.0",
    "@svgdotjs/svg.js": "^3.0.16",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.167",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-test-renderer": "^17.0.0",
    "@types/slick-carousel": "^1.6.34",
    "@types/webpack": "^4.41.26",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "babel-loader": "^8.2.2",
    "browser-sync": "^2.26.13",
    "css-loader": "^5.0.1",
    "css-minimizer-webpack-plugin": "^1.2.0",
    "eslint": "^7.17.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.22.0",
    "file-loader": "^6.2.0",
    "fomantic-ui": "^2.8.7",
    "gulp": "^4.0.2",
    "highcharts": "^8.2.2",
    "highcharts-react-official": "^3.0.0",
    "jest": "^26.6.3",
    "less-loader": "^7.2.1",
    "lodash": "^4.17.20",
    "mini-css-extract-plugin": "^1.3.4",
