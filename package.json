{
  "name": "alagodich.github.io",
  "version": "2.2.0",
  "description": "Personal blog.",
  "main": "index.html",
  "scripts": {
    "start": "(cd docs && JEKYLL_ENV=development bundle exec jekyll serve --draft)",
    "lint": "eslint --config ./.eslintrc.json --ext .js,.jsx,.ts,.tsx client/ server/",
    "test": "jest --errorOnDeprecated --logHeapUsage --verbose",
    "test:watch": "npm run test -- --watch",
    "build": "rm -rf ./docs/public && webpack --config webpack.config.production.js",
    "build:watch": "rm -rf ./docs/public && webpack --config webpack.config.dev.js",
    "fomantic": "gulp build-fomantic",
    "build:all": "npm run fomantic && npm run lint && npm run test && npm run build",
    "all": "npm outdated && npm run lint && npm run test && npm run build",
    "browser-sync": "browser-sync start --proxy localhost:4000 --files docs/_site/**",
    "cypress": "cypress open",
    "postinstall": "rm -f node_modules/preact/*/preact.d.ts",
    "node": "ts-node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alagodich/alagodich.github.io.git"
  },
  "author": "alagodich",
  "license": "ISC",
  "homepage": "https://github.com/alagodich/alagodich.github.io#readme",
