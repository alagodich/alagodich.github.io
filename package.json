    "semantic-ui-react": "^2.0.2",
    "slick-carousel": "^1.8.1",
    "style-loader": "^2.0.0",
    "tinify": "^1.6.0-beta.2",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.14",
    "tslib": "^2.1.0",
